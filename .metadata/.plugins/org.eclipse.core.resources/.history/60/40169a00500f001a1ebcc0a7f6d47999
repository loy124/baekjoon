import java.util.Scanner;
public class Main{
	public static void main(String args[]){
		Scanner scanner = new Scanner(System.in);
		double a, b;
		a = scanner.nextInt();
		b = scanner.nextInt();
		double divide = a / b;
		System.out.println(divide);
	}
}
